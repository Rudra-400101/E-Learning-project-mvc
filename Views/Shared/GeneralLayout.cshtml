    <script>

        //moving elements with mouse cursor
        document.querySelector("body").addEventListener("mousemove", function (e) {
            let box = document.querySelector(".moveOnMouseCursor");
            let box2 = document.querySelector(".moveOnMouseCursor2");
            let box3 = document.querySelector(".moveOnMouseCursor3");
            box.style.top = e.y+8  + "px";
            box.style.left = e.x + "px";

            box2.style.top = e.y+8 + "px";
            box2.style.left = e.x + "px";

            box3.style.top = e.y+8  + "px";
            box3.style.left = e.x + "px";
        })
    </script>
